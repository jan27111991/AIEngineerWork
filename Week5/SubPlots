import matplotlib.pyplot as plt

fig, axs = plt.subplots(1,2)
fig.suptitle("Test Execution and Defect Distrubution Charts")
fig.set_facecolor('lightgrey')

data =  [12, 15, 20, 18, 22, 30, 25, 16, 19, 28, 24, 14]
axs[0].hist(data,9 bins=5, color='skyblue', edgecolor='black',alpha = 0.3)
axs[0].set_title("Distruction of Test Execution Times")
axs[0].set_xlabel("Duration (seconds)")
axs[0].set_ylabel("Number of Test Cases")


x = ['High','Medium','Low']


